{
  "coc.preferences.formatOnSaveFiletypes": [
    "json",
    "go",
    "css",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.semi": false,
  "go.goplsOptions": {
    "completeUnimported": true,
    "usePlaceholders": true
